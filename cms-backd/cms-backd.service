[Unit]
Description=CMS backend daemon
Requires=cms-fsd.service cms-postd.service
After=cms-fsd.service cms-postd.service

[Service]
Type=notify
NotifyAccess=main
ExecStart=/opt/cms/bin/cms-backd
ExecReload=/bin/kill -HUP $MAINPID
StandardOutput=journal
StandardError=journal
Restart=on-failure
RestartSec=1
User=cms-backd
Group=cms-backd
Nice=0
#Environment=RUST_BACKTRACE=1
